{
  "name": "davivienda_database",
  "version": "0.0.1",
  "description": "",
  "author": "josue-leon",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"utils/**/*.ts\" \"schema-api/**/*.ts\"",
    "start": "nest start",
    "start:dev": "cross-env NODE_ENV=dev nest start --watch",
    "start:debug": "cross-env NODE_ENV=dev nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:e2e:report": "jest --config ./test/jest-e2e.report.json",
    "db:seed": "tsx prisma/seed.ts",
    "gmap": "tsx schema-api/generar.ts",
    "prepare": "husky",
    "lint-staged": "lint-staged"
  },
  "dependencies": {
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/helmet": "^13.0.2",
    "@fastify/static": "^8.3.0",
    "@nestjs/common": "^11.1.8",
    "@nestjs/core": "^11.1.8",
    "@nestjs/platform-fastify": "^11.1.8",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@prisma/client": "^6.18.0",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "nodemailer": "^7.0.10",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.38.0",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/schematics": "^11.0.9",
    "@nestjs/testing": "^11.1.8",
    "@openapitools/openapi-generator-cli": "2.25.0",
    "@swc/cli": "^0.7.8",
    "@swc/core": "^1.14.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.2",
    "@types/nodemailer": "^7.0.3",
    "@types/supertest": "^6.0.3",
    "@usebruno/cli": "2.13.2",
    "cross-env": "^10.1.0",
    "dotenv-expand": "^12.0.3",
    "esbuild": "^0.25.11",
    "esbuild-node-externals": "^1.18.0",
    "esbuild-plugin-alias": "^0.2.1",
    "esbuild-plugin-alias-path": "^2.0.2",
    "esbuild-plugin-copy": "^2.1.1",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-html-reporter": "^4.3.0",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "prisma": "^6.18.0",
    "rimraf": "^6.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^src/(.*)$": "<rootDir>/$1",
      "^prisma/(.*)$": "<rootDir>/../prisma/$1",
      "^utils/(.*)$": "<rootDir>/../utils/$1",
      "^envs$": "<rootDir>/../envs.ts"
    }
  }
}